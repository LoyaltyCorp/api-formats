{
  "name": "eoneopay/api-formats",
  "description": "Api formats from EoneoPay.",
  "keywords": ["api", "formats", "eoneopay"],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": ">=7.1",
    "symfony/http-kernel": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": { "EoneoPay\\ApiFormats\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\EoneoPay\\ApiFormats\\": "tests/" }
  },
  "optimize-autoloader": true
}
