{
  "name": "eoneopay/api-formats",
  "description": "Api formats from EoneoPay.",
  "keywords": ["api", "formats", "eoneopay"],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master",
    "symfony/psr-http-message-bridge": "^1.0",
    "zendframework/zend-diactoros": "^1.7",
    "eoneopay/utils": "dev-master"
  },
  "require-dev": {
    "illuminate/http": "^5.5",
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0",
    "guzzlehttp/psr7": "^1.4",
    "illuminate/support": "^5.5"
  },
  "suggest": {
    "illuminate/http": "To use the Laravel implementation",
    "illuminate/support": "To use Laravel service provider"
  },
  "autoload": {
    "psr-4": { "EoneoPay\\ApiFormats\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\EoneoPay\\ApiFormats\\": "tests/" }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git@bitbucket.org/eoneopay/utils.git"
    }
  ],
  "optimize-autoloader": true,
  "minimum-stability": "dev",
  "prefer-stable": true
}
